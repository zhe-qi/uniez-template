{
  "pages": [
    {
      "path": "pages/index/index",
      "type": "home",
      "name": "Index",
      "style": {
        "navigationBarTitleText": "首页",
        "navigationStyle": "custom"
      },
      "meta": {
        "ignoreAuth": true
      }
    },
    {
      "path": "pages/my/my",
      "type": "page",
      "name": "My",
      "style": {
        "navigationBarTitleText": "我的",
        "navigationStyle": "custom"
      },
      "meta": {
        "ignoreAuth": true
      }
    },
    {
      "path": "pages/test-page/test-page",
      "type": "page",
      "name": "TestPage",
      "style": {
        "navigationBarTitleText": "测试页面",
        "navigationStyle": "custom"
      },
      "meta": {
        "ignoreAuth": true
      }
    },
    {
      "path": "pages/theme/theme",
      "type": "page",
      "name": "Theme",
      "style": {
        "navigationBarTitleText": "动态主题",
        "navigationStyle": "custom"
      },
      "meta": {
        "ignoreAuth": true
      }
    },
    {
      "path": "pages/webview/webview",
      "type": "page",
      "name": "Webview",
      "style": {
        "navigationBarTitleText": ""
      }
    }
  ],
  "preloadRule": {
    "pages-sub/list/list": {
      "network": "all",
      "packages": [
        "pages-sub"
      ]
    }
  },
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "tabBar": {
    "custom": true,
    "color": "#999",
    "selectedColor": "#000",
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "首页"
      },
      {
        "pagePath": "pages/my/my",
        "text": "我的"
      }
    ]
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue",
      "^z-tabs": "@zxlee/z-tabs/components/z-tabs/z-tabs.vue"
    }
  },
  "subPackages": [
    {
      "root": "pages-sub",
      "pages": [
        {
          "path": "list/list",
          "type": "page",
          "name": "List",
          "style": {
            "navigationBarTitleText": "",
            "navigationStyle": "custom"
          },
          "meta": {
            "ignoreAuth": true
          }
        }
      ]
    }
  ]
}
